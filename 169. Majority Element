public class Solution {
    public int majorityElement(int[] nums) {
        if(nums == null || nums.length == 0){
            return -1;
        }
        int majority = nums[0];
        int count = 0;
        for(int i = 0; i < nums.length; i ++){
            if(majority == nums[i]){
                count ++;
            } else {
                count --;
            }
            if(count < 0){
                count = 0;
                majority = nums[i];
            }
        }
        return majority;
    }
}

//line 14是“小于”不是“小于等于”，line 15记得把count清零


