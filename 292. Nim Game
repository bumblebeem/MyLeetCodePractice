public class Solution {
    public boolean canWinNim(int n) {
        if(n % 4 != 0){
            return true;
        }
        return false;
    }
}


class Solution(object):
    def canWinNim(self, n):
        """
        :type n: int
        :rtype: bool
        """
        return n % 4 != 0;
        
