public class Solution {
    public int missingNumber(int[] nums) {
        int res = 0;
        for(int i = 0; i < nums.length; i ++){
            res ^= nums[i] ^ i;
        }
        return res ^ nums.length;
    }
}


public class Solution {
    public int missingNumber(int[] nums) {
        int sum = nums.length;
        for(int i = 0; i < nums.length; i ++){
            sum += i - nums[i];
        }
        return sum;
    }
}


