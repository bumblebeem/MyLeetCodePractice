class Solution {
public:
    int maxProfit(vector<int>& prices) {
        int max_profit = 0;
        if(prices.size() == 0){
            return max_profit;
        }
        for(int i = 0; i < prices.size() - 1; i ++){
            if(prices[i + 1] - prices[i] > 0){
                max_profit += prices[i + 1] - prices[i];
            }
        }
        return max_profit;
    }
};

//don't forget line 5-7
